<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequence for Customer -->
        <record id="seq_customer_contract" model="ir.sequence">
            <field name="name">Customer Contract</field>
            <field name="code">contract.contract</field>
            <field name="prefix">CCON-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequence for Vendor -->
        <record id="seq_vendor_contract" model="ir.sequence">
            <field name="name">Vendor Contract</field>
            <field name="code">contract.contract</field>
            <field name="prefix">VCON-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
